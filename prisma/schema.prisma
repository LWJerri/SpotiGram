generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DATABASE_URL")
}

model Playlist {
  id         String  @id
  snapshotId String
  track      Track[]

  @@map("playlists")
}

model Track {
  id         String    @id
  addedAt    DateTime? @default(now())
  playlistId String
  playlist   Playlist  @relation(fields: [playlistId], references: [id])

  @@map("tracks")
}
